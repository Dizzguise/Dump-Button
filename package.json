{
  "name": "dump-button",
  "version": "1.0.0",
  "description": "This application allows you to manage live streams, seamlessly switching between live content from OBS and a pre-recorded dump video. It's designed to output directly to YouTube but can be configured for other streaming platforms.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron ."
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "dotenv": "^16.4.5",
    "node-media-server": "^2.6.2",
    "pino": "^8.19.0",
    "promise.rpc": "^1.0.4"
  },
  "devDependencies": {
    "electron": "^29.1.6"
  },
  "build": {
    "appId": "button.dump",
    "productName": "DumpButton",
    "mac": {
      "target": "dmg",
      "icon": "icons/mac/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "icons/win/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "icons/png/"
    }
  }
}
